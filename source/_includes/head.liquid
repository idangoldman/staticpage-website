<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  {% seo %}
  {% favicon %}
  {% feed_meta %}

  {% comment %}
    {% feed_meta %}
  {% endcomment %}

  {% if page.stylesheet %}
    <link rel="stylesheet" href="{{ page.stylesheet }}" />
  {% elsif layout.stylesheet %}
    <link rel="stylesheet" href="{{ layout.stylesheet }}" />
  {% else %}
    <link rel="stylesheet" href="{{ '/assets/css/default.css' }}" />
  {% endif %}

  {% if site.ENV == "production" and site.GA_ID %}
    {% include 3rd_party/google_analytics.liquid id=site.GA_ID %}
  {% endif %}
</head>
